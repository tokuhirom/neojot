{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "context": "local",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "dialog:allow-message",
    "fs:allow-app-read",
    "fs:allow-appdata-write-recursive",
    "fs:allow-appdata-read-recursive",
    "autostart:allow-is-enabled",
    "autostart:allow-enable",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "$APPDATA"
        },
        {
          "path": "$APPDATA/*"
        },
        {
          "path": "$APPDATA/.git"
        },
        {
          "path": "$APPDATA/data/*"
        },
        {
          "path": "$APPDATA/calendar/*"
        },
        {
          "path": "$APPDATA/archived/*"
        },
        {
          "path": "$APPDATA/images/*"
        }
      ]
    },
    "notification:default"
  ],
  "platforms": [
    "linux",
    "macOS",
    "windows",
    "android",
    "iOS"
  ]
}